sentence = 'This is a common interview question'
dict1 = {}
for char in sentence:
    if char in dict1:
        dict1[char] += 1
    else:
        dict1[char] = 1

print(dict1)
value = dict1.values()
maximum_values = [k for k, v in dict1.items() if v == max(value) and k != ' ']
print(maximum_values)
